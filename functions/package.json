    "firebase-admin": "^13.0.2",
    "firebase-functions": "^6.1.0",
    "puppeteer": "^24.26.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.4",
    "@types/node": "^24.9.1",
    "typescript": "^5.6.3"
  },
  "private": true
